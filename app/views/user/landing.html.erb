<div class="title"><h1>登陆界面</h1></div>
<div class="Login_information">
  <%= form_tag "/create_login_session" do %>
    <dl class="form">
      <dt class="Writing_style">
        <%= label_tag "用户名" %>
      </dt>
      <dd class="user_name_box">
        <%= text_field_tag :name, params[:name] %>
      </dd>
    </dl>
    <dl class="form">
      <dt class="Writing_style">
        <%= label_tag "密 码" %>
      </dt>
      <dd class="user_name_box">
        <%= password_field_tag :password, params[:password] %>
      </dd>
    </dl>
      </div>
    <p class="landing_bnt"> <%= submit_tag "登录", :class => "button primary" %> </p>
<% end %>
<div class="bnt"><button  onclick="open_register()">注册</button></div>